{
  "extends": [
    "../../tslint.json"
  ],
  "rulesDirectory": [
    "node_modules/codelyzer"
  ],
  "rules": {
    "experimentalDecorators": true,

    "array-type": ["array"],
    "directive-selector": [
      true,
      "attribute",
      "matKeyboard",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "mat-keyboard",
      "kebab-case"
    ],
    "pipe-naming": [
      true,
      "camelCase",
      "matKeyboard"
    ]
  }
}